<%= link_to 'About ConsignNYC', "/about", :class => 'navbar-link' %> |
<%= link_to 'Consignment Stores', businesses_path, :class => 'navbar-link' %> |
<%= link_to 'Types of Stores', biztags_path, :class => 'navbar-link' %> |
<%= link_to 'Browse Store Photos', photos_path, :class => 'navbar-link' %> |
<%= link_to 'Browse Designers', designers_path, :class => 'navbar-link' %> | 
<%= link_to 'Advanced Search', "/search", :class => 'navbar-link' %> | 
<% if user_signed_in? %>

  <% if (current_user.name != nil && current_user.name != "" ) %>
    Welcome, <strong><%= current_user.name %><%#= current_user.email %></strong>! |
  <% else %>
    Welcome! |
  <% end %>

  <!-- <%#= link_to 'Edit Settings', edit_user_registration_path, :class => 'navbar-link' %> | -->

  <% if (current_user.user_name != nil && current_user.name != nil && current_user.user_name != "" && current_user.name != "") %> 
    <%= link_to "Profile", current_user, :class => 'navbar-link' %> | 

  <% else  %>
    <%= link_to "Complete your profile", edit_user_path(current_user), :class => 'navbar-link'  %>
  <% end %>

  <%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'%>

<% else %>
  <%= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %> |
  <%= link_to "Login", new_user_session_path, :class => 'navbar-link'  %>

<% end %>







